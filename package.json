{
	"name": "watermelon",
	"description": "Watermelon, find all the historical context of your code",
	"version": "0.0.1",
	"publisher": "Watermelon Tools Inc",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/orgs/watermelontools/"
	},
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:watermelon.start",
		"onWebviewPanel:watermelon"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "watermelon.start",
				"title": "Start watermelon",
				"category": "watermelon"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.63.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"@octokit/core": "^3.5.1",
		"@octokit/rest": "^18.12.0",
		"@vscode/test-electron": "^2.1.2",
		"github-graphql-api": "^1.0.6"
	}
}
